{
  "name": "di-medical-managment",
  "version": "1.0.0",
  "description": "Di medical corporativo managment platform",
  "main": "index.js",
  "author": "Kevin Tapia",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "dev": "nodemon ./src/index.ts",
    "lint": "npx ts-standard --fix",
    "start": "node ./dist/src/index.js",
    "build": "npm run build:clean && npm run build:tsc && npm run build:di",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "build:di": "copy 'src/**/*.{json,yaml,html,png}' dist/src",
    "build:clean": "rm -r dist; exit 0",
    "dev:warehouse:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules  ./src/apps/backoffice/backend/start.ts",
    "test:warehouse:backend:sucursal": "NODE_ENV=test cucumber-js -p sucursal",
    "test:warehouse:backend:user": "NODE_ENV=test cucumber-js -p user",
    "test:warehouse:backend:truck": "NODE_ENV=test cucumber-js -p truck",
    "test:warehouse:backend:client": "NODE_ENV=test cucumber-js -p client",
    "test:warehouse:backend:survey": "NODE_ENV=test cucumber-js -p survey",
    "test:unit": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@prisma/client": "5.5.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "glob": "^10.4.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "method-override": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "node-dependency-injection": "^2.6.11",
    "typescript": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.8.0",
    "@types/bcrypt": "^5.0.1",
    "@types/body-parser": "^1.19.3",
    "@types/cors": "^2.8.17",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/method-override": "^0.0.35",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.14.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
